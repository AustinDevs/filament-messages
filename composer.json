{
    "name": "austindevs/filament-messages",
    "description": "Filament Messages is a powerful messaging plugin for FilamentPHP. It provides an easy-to-use interface for real-time messaging within Filament admin panels.",
    "keywords": ["laravel", "framework", "admin-panel", "messages", "chat", "filament"],
    "homepage": "https://github.com/AustinDevs/filament-messages",
    "support": {
        "issues": "https://github.com/AustinDevs/filament-messages/issues",
        "source": "https://github.com/AustinDevs/filament-messages"
    },
    "type": "library",
    "license": "MIT",
    "version": "2.0.0",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "filament/filament": "^4.0",
        "laravel/framework": "^11.31|^12.0",
        "phpunit/phpunit": "^11.0.1",
        "filament/spatie-laravel-media-library-plugin": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "AustinDevs\\FilamentMessages\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rasel",
            "email": "raselbabu34.bd@gmail.com"
        },
        {
            "name": "Kevin Colten",
            "email": "kevincolten@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AustinDevs\\FilamentMessages\\FilamentMessagesServiceProvider"
            ],
            "aliases": {
                "FilamentMessages": "AustinDevs\\FilamentMessages\\Facades"
            }
        }
    }
}
